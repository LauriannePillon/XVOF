{
  "time-management": {
    "final-time": 5e-06,
    "initial-time-step": 1e-08,
    "constant-time-step": true
  },
  "geometry": {
    "section": 3.141592653589793e-06
  },
  "matter": {
    "initialization": {
      "initial-velocity": 0.0,
      "init-thermo": "Cuivre_InitThermo.json"
    },
    "equation-of-state": {
      "name": "Mie-Gruneisen",
      "coefficients": "Cuivre_MieGruneisen.json"
    },
    "failure": {
      "failure-criterion": {
        "name": "HalfRodComparison",
        "value": 25
      },
      "failure-treatment": {
        "name": "Enrichment",
        "value": 0.5,
        "type-of-enrichment": "Hansbo",
        "lump-mass-matrix": "somme"
      }
    }
  },
  "boundary-conditions": {
    "right-boundary": {
      "type": "pressure",
      "bc-law": "Constant",
      "value": 0.0
    },
    "left-boundary": {
      "type": "velocity",
      "bc-law": "Constant",
      "value": -30.0
    }
  },
  "numeric-parameters": {
    "linear-pseudo": 0.2,
    "quadratic-pseudo": 1.5,
    "cfl": 0.5,
    "cfl-pseudo": 0.0
  },
  "output": {
    "number-of-images": 500,
    "dump-images": false,
    "database": {
      "identifier": "AllFieldsDb",
      "path": "all_fields.hdf5",
      "iteration-period": 1
    }
  }
}