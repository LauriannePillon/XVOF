<?xml version="1.0" encoding="UTF-8"?>
<data>
    <time-management>
        <final-time>5e-06</final-time>
        <initial-time-step>1e-08</initial-time-step>
        <constant-time-step>True</constant-time-step>
    </time-management>
    <geometry>
        <section>0.000003141592653589793</section>
        <!--<initial-interface-position>3.5e-03</initial-interface-position>-->
    </geometry>
    <matter>

            <initialization>
                <initial-velocity>0.</initial-velocity>
                <init-thermo>Cuivre_InitThermo.json</init-thermo>
            </initialization>
            <equation-of-state>
                <name>Mie-Gruneisen</name>
                <coefficients>Cuivre_MieGruneisen.json</coefficients>
            </equation-of-state>
            <failure>
                <failure-criterion>
                    <name>HalfRodComparison</name>
                    <value>25</value>
                </failure-criterion>
                <failure-treatment>
                    <name>Enrichment</name>
                    <value>0.5</value>
                    <type-of-enrichment>Hansbo</type-of-enrichment>
                    <lump-mass-matrix>somme</lump-mass-matrix>
                </failure-treatment>
            </failure>
    </matter>

    <boundary-conditions>
        <right-boundary>
            <type>pressure</type>
            <bc-law>Constant</bc-law> <!--ConstantPressure, MarchTablePressure, TwoStepsPressure, CreneauRampPressure, RampPressure -->
            <value>0</value>
        </right-boundary>
        <left-boundary>
            <type>velocity</type> <!--pressure | velocity-->
            <bc-law>Constant</bc-law>
            <value>-30</value>
        </left-boundary>
    </boundary-conditions>
    <numeric-parameters>
        <linear-pseudo>0.2</linear-pseudo>
        <quadratic-pseudo>1.5</quadratic-pseudo>
        <cfl>0.5</cfl>
        <cfl-pseudo>0.</cfl-pseudo>
        <!--<external-solver-library>vnr_internal_energy_evolution.so</external-solver-library>-->
    </numeric-parameters>
    <output>
        <number-of-images>500</number-of-images>
        <dump-images>false</dump-images> <!-- to save images -->
        <database>
            <identifier>AllFieldsDb</identifier>
            <path>all_fields.hdf5</path>
            <iteration-period>1</iteration-period>
        </database>
    </output>
</data>
