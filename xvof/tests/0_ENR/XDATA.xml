                <failure-criterion>
                    <name>HalfRodComparison</name> <!-- MaximalStress, MinimumPressure, HalfRodComparison, Damage-->
                    <value>25</value>
                </failure-criterion>
                <failure-treatment>
                    <name>Enrichment</name>
                    <value>0.5</value>
                    <type-of-enrichment>Hansbo</type-of-enrichment> <!-- Hansbo, Moes (pas complet)-->
                    <lump-mass-matrix>somme</lump-mass-matrix> <!-- none, somme, menouillard-->
                </failure-treatment>
