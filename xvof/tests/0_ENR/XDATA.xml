<?xml version="1.0" encoding="UTF-8"?>
<data>
    <time-management>
        <final-time>5e-06</final-time>
        <initial-time-step>1e-08</initial-time-step>
        <constant-time-step>True</constant-time-step>
    </time-management>
    <geometry>
        <section>0.000003141592653589793</section>
        <!--<initial-interface-position>3.5e-03</initial-interface-position>-->
    </geometry>
    <matter>


        <!--<target>-->
            <initialization>
                <initial-velocity>0.</initial-velocity>
                <init-thermo>Cuivre_InitThermo.json</init-thermo>
            </initialization>
            <equation-of-state>
                <name>Mie-Gruneisen</name>
                <coefficients>Cuivre_MieGruneisen.json</coefficients>
            </equation-of-state>
