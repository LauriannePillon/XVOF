                            <!--<separation-at-point-1>0.5e-09</separation-at-point-1>-->
                            <!--&lt;!&ndash;<stress-at-point-2>0.2e+09</stress-at-point-2> &lt;!&ndash; Deuxième rupture de pente &ndash;&gt;&ndash;&gt;-->
                            <!--&lt;!&ndash;<separation-at-point-2>1.e-07</separation-at-point-2>&ndash;&gt;-->
                            <!--<critical-separation>1.e-7</critical-separation>-->
