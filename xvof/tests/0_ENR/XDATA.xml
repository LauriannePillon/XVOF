                        <!--<unloading-model>-->
                            <!--<name>ProgressiveUnloading</name>-->
                            <!--<slope>1.e+16</slope>-->
                        <!--</unloading-model>-->
                    <!--</cohesive-model>-->
                <!--</damage-treatment>-->
            </failure>
        <!--</target>-->
    </matter>

    <boundary-conditions>
        <right-boundary>
            <type>pressure</type>
            <bc-law>Constant</bc-law> <!--ConstantPressure, MarchTablePressure, TwoStepsPressure, CreneauRampPressure, RampPressure -->
            <value>0</value>
        </right-boundary>
        <left-boundary>
            <type>velocity</type> <!--pressure | velocity-->
            <bc-law>Constant</bc-law>
            <value>-30</value>
        </left-boundary>
    </boundary-conditions>
    <numeric-parameters>
        <linear-pseudo>0.</linear-pseudo>
        <quadratic-pseudo>0.</quadratic-pseudo>
        <cfl>0.5</cfl>
        <cfl-pseudo>0.</cfl-pseudo>
    </numeric-parameters>
    <output>
        <number-of-images>500</number-of-images>
        <dump-images>false</dump-images> <!-- to save images -->
        <database>
            <identifier>AllFieldsDb</identifier>
            <path>all_fields.hdf5</path>
            <iteration-period>1</iteration-period>
        </database>
    </output>
</data>
