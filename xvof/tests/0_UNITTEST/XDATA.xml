<?xml version="1.0" encoding="UTF-8"?>
<data>
    <time-management>
        <final-time>5e-06</final-time>
        <initial-time-step>0.5e-09</initial-time-step>
        <constant-time-step>True</constant-time-step>
    </time-management>

    <geometry>
        <section>0.000003141592653589793</section>
        <initial-interface-position>1.5e-03</initial-interface-position>
    </geometry>

    <matter>
        <projectile>
            <initialization>
                <initial-velocity>0.</initial-velocity>
                <init-thermo>Cuivre_InitThermo_GaiaLong.json</init-thermo>
            </initialization>
            <equation-of-state>
                <name>Mie-Gruneisen</name>
                <coefficients>Cuivre_MieGruneisen.json</coefficients>
            </equation-of-state>
            <rheology>
                <coefficients>Cuivre_EPP.json</coefficients>
                <elasticity-model>Linear</elasticity-model>
                <plasticity-criterion>VonMises</plasticity-criterion>
                <plasticity-model>EPP</plasticity-model>
            </rheology>
        </projectile>

        <target>
            <initialization>
                <initial-velocity>0.</initial-velocity>
                <init-thermo>Cuivre_InitThermo_GaiaLong.json</init-thermo>
            </initialization>
            <equation-of-state>
                <name>Mie-Gruneisen</name>
                <coefficients>Cuivre_MieGruneisen.json</coefficients>
            </equation-of-state>
            <rheology>
                <coefficients>Cuivre_EPP.json</coefficients>
                <elasticity-model>Linear</elasticity-model>
                <plasticity-criterion>VonMises</plasticity-criterion>
                <plasticity-model>EPP</plasticity-model>
            </rheology>
            <failure>
                <failure-criterion>
                    <name>MaximalStress</name>
                    <value>5.e+09</value>
                </failure-criterion>
                <failure-treatment>
                    <name>Enrichment</name>
                    <value>0.5</value>
                    <type-of-enrichment>Hansbo</type-of-enrichment>
                    <lump-mass-matrix>somme</lump-mass-matrix>
                </failure-treatment>
            </failure>
        </target>
    </matter>

    <boundary-conditions>
        <left-boundary>
            <type>pressure</type>
            <bc-law>TwoStep</bc-law>
            <value1>10.e+9</value1>
            <value2>0.</value2>
            <time-activation>1.e-06</time-activation>
        </left-boundary>
        <right-boundary>
            <type>pressure</type>
            <bc-law>constant</bc-law>
            <value>0.</value>
        </right-boundary>
    </boundary-conditions>

    <numeric-parameters>
        <linear-pseudo>0.2</linear-pseudo>
        <quadratic-pseudo>2.</quadratic-pseudo>
        <cfl>0.34</cfl>
        <cfl-pseudo>0.</cfl-pseudo>
    </numeric-parameters>

    <output>
        <number-of-images>500</number-of-images>
        <dump-images>false</dump-images> <!-- bool = "true" to save images, else "false" -->
        <database>
            <identifier>AllFieldsDb</identifier>
            <path>all_fields.hdf5</path>
            <iteration-period>20</iteration-period>
        </database>
    </output>
</data>
